             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App.Themes.TealTheme">

	<!--PrimaryColor-->
	<Color x:Key="PrimaryColor">#00695C</Color>

	<!--AccentColor-->
	<Color x:Key="AccentColor">#439889</Color>

	<!--ErrorColor-->
	<Color x:Key="ErrorColor">#EE6002</Color>

	<!--BackgroundMainColor-->
